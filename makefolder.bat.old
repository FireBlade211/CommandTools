@echo off
REM Copies all the programs into one folder along with all their dependencies

SET "CommandToolsAppDir=.\AllApps"

REM Recreate the directory
MKDIR "%CommandToolsAppDir%" >nul 2>&1
XCOPY /EY ".\sysinfo\bin\Release\net8.0-windows\*" "%CommandToolsAppDir%\*" >nul 2>&1
XCOPY /EY ".\process\bin\Release\net8.0-windows\*" "%CommandToolsAppDir%\*" >nul 2>&1
XCOPY /EY ".\run\bin\Release\net8.0-windows\*" "%CommandToolsAppDir%\*" >nul 2>&1

DEL "%CommandToolsAppDir%\WinInteropUtils.*"

ECHO Script Finished!